
<table class="table table-striped">
<thead>
	<tr>
		<th>Product Name</th>
		<th>Product Image</th>
		<th>Product Detail</th>
	</tr>
</thead>
<tbody>

{{#each list as |product|}}
	<tr>
		<td>{{product.productTitle}} {{product.title}}</td>
		<td><img src="{{product.productImg}}" alt="" width="100"></td>
		<td>
			{{product.productDetail}}
		</td>
	</tr>
	{{/each}}	
</tbody>	
</table>

{{yield}}
